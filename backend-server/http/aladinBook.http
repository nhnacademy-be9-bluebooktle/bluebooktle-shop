### 최상위 카테고리 등록 1
POST {{categoryUrl}}
Content-Type: application/json

{
  "rootCategoryName": "카테고리1",
  "childCategoryName": "카테고리1_1"
}

### 카테고리1의 하위 카테고리 등록 성공
POST {{categoryUrl}}/{{category1}}
Content-Type: application/json

{
  "name": "카테고리1_2"
}

### 카테고리1_1의 하위 카테고리 등록
POST {{categoryUrl}}/{{category1_1}}
Content-Type: application/json

{
  "name": "카테고리1_1_1"
}


### 알라딘 도서목록 제목으로 검색
GET {{baseUrl}}/aladin-search?keyword=창문 넘어 도망친 100세 노인
Accept: application/json

### 알라딘 도서 isbn으로 검색
GET {{baseUrl}}/select?isbn=9791186560655
Accept: application/json

### 알라딘 도서 등록
POST {{baseUrl}}
Content-Type: application/json

{
  "isbn": "9791186560655",
  "stock": 999,
  "isPackable": true,
  "state": "AVAILABLE",
  "categoryIdList": [
    2,
    3
  ],
  "tagIdList": [
    1
  ]
}

